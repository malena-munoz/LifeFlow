@model IEnumerable<lifeflow_api.Models.Database.Sintomas>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PruebaEmbarazo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Emociones)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SintomasCuerpo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Molestias)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FluidoFemenino)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PruebaEmbarazo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Emociones)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SintomasCuerpo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Molestias)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FluidoFemenino)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
